root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
trim_trailing_whitespace = true
insert_final_newline = true
max_line_length = 160
tab_width = 4

[*.{kt,kts}]
disabled_rules = no-wildcard-imports, import-ordering, comment-spacing

[*Test.kt]
max_line_length = 200